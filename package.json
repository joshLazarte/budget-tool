{
  "name": "budget-assist",
  "version": "1.0.0",
  "description": "Budget tool",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "json:server": "json-server --watch api/db.json --host 0.0.0.0 --port 8081",
    "watch:js": "webpack --watch --mode development",
    "build:js": "webpack --mode production",
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "concat:css": "concat -o public/css/style.concat.css public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" public/css/style.concat.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Josh Lazarte",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.18.1",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.3",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "fs-extra": "^7.0.1",
    "html-pdf": "^2.2.0",
    "isomorphic-fetch": "^2.2.1",
    "json-server": "^0.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "^9.4.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.0.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2"
  }
}
